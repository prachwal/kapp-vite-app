{
  "name": "kapp-vite-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "bash ./scripts/dev-start.sh",
    "build": "bash ./scripts/build-full.sh",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "api": "bash ./scripts/start-api.sh",
    "api:start": "bash ./scripts/start-api.sh",
    "api:kill": "bash ./scripts/kill-api.sh",
    "api:logs": "tail -n 200 -f logs/api-server.log",
    "api:v2:build": "tsc -p api/tsconfig.json",
    "api:v2:start": "PORT=3000 node api/dist/index.js",
    "api:v2:serve": "PORT=3000 node api/dist/index.js"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "6.1.3",
    "@tsconfig/svelte": "5.0.5",
    "@types/express": "5.0.3",
    "@types/node": "24.3.0",
    "svelte": "5.38.2",
    "svelte-check": "4.3.1",
    "typescript": "5.9.2",
    "vite": "7.1.3"
  },
  "dependencies": {
    "compression": "1.8.1",
    "cors": "2.8.5",
    "express": "5.1.0",
    "winston": "3.17.0",
    "sirv": "3.0.1"
  }
}