{
    "functions": {
        "api/dist/vercel.js": {
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/dist/client/assets/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/dist/vercel.js"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/client/$1"
        }
    ]
}