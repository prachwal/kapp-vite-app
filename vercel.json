{
    "functions": {
        "api/vercel.ts": {
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/dist/assets/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/vercel.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/$1"
        }
    ]
}